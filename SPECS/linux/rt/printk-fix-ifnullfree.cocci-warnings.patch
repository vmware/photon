--- a/kernel/printk/printk.c
+++ b/kernel/printk/printk.c
@@ -1466,10 +1466,8 @@ static int syslog_print_all(char __user
 	if (clear && !seq)
 		syslog_clear();
 
-	if (text)
-		kfree(text);
-	if (msgbuf)
-		kfree(msgbuf);
